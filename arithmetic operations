#include<stdio.h>
int main(){
    int a,b, result;
    printf("enter the first number\n");
    scanf("%d",&a);
    printf("enter the second number\n");
    scanf("%d",&b);
    result = a+b;
    printf("a+b is %d\n",result);
    result = a-b;
    printf("a-b is %d\n",result);
    result = a*b;
    printf("a*b is %d\n",result);
    result = a/b;
    printf("a/b is %d\n",result);
    result = a%b;
    printf("a%b is %d\n",result);
    return 0;
}
